-- Creating an Employee table
CREATE TABLE Employee (
    id INT ,
    name VARCHAR(50),
    age INT,
    salary INT
);


-- Inserting data in employee table
insert into Employee (id,name,age,salary)
values(1,"Arif Iqubal",22,50000),
(2,"Md Sagir",22,50000),
(3,"Jeeshan", 21,55000),
(4,"Rahul",34,67000),
(5,"Sohan",43,70000);


-- Printing all the records from employee table
Select * from Employee;


-- Updating the salary of employee by id
update Employee
set salary = 100000
where id = 1;


-- Deleting the record of employee by id
delete from Employee
where id = 3;

-- Searching employee by  name
select * from Employee
Where name = "Arif Iqubal";